        lng: position.coords.longitude,\n        });\n      });\n    }\n  };\n\n  useEffect(() => {\n    const fetchApiKey = async () => {\n      const { data } = await axios(\"/api/keys/google\", {\n        headers: { Authorization: `BEARER ${userInfo.token}` },\n      });\n      setGoogleApiKey(data.key);\n      getUserCurrentLocation();\n    };\n\n    fetchApiKey();\n    ctxDispatch({\n      type: \"SET_FULLBOX_ON\",\n    });\n  }, [ctxDispatch, userInfo.token]);\n\n  const onLoad = (map) => {\n    mapRef.current = map;\n  };\n\n  const onIdle = () => {\n    setLocation({\n      lat: mapRef.current.center.lat(),\n      lng: mapRef.current.center.lng(),\n    });\n  };\n\n  const onLoadPlaces = (place) => {\n    placeRef.current = place;\n  };\n\n  const onPlacesChanged = () => {\n    const place = placeRef.current.getPlaces()[0].geometry.location;\n    setCenter({ lat: place.lat(), lng: place.lng() });\n    setLocation({ lat: place.lat(), lng: place.lng() });\n  };\n\n  const onMarkLoad = (marker) => {\n    markerRef.current = marker;\n  };\n\n  const onConfirm = () => {\n    const places = placeRef.current.getPlaces() || [{}];\n    ctxDispatch({\n      type: \"SAVE_SHIPPING_ADDRESS_MAP_LOCATION\",\n      payload: {\n        lat: location.lat,\n        lng: location.lng,\n        address: places[0].formatted_address,\n        name: places[0].name,\n        vicinity: places[0].vicinity,\n        googleAddressId: places[0].id,\n      },\n    });\n    toast.success(\"Location selected successfully.\");\n    navigate(\"/shipping\");\n  };\n\n  return (\n    <div className=\"full-box\">\n      <LoadScript libraries={libs} >\n        <GoogleMap\n          id=\"sample-map\"\n          mapContainerStyle={{ height: \"100%\", width: \"100%\" }}\n          center={center}\n          zoom={15}\n          onLoad={onLoad}\n          onIdle={onIdle}\n        >\n          <StandaloneSearchBox\n            onLoad={onLoadPlaces}\n            onPlacesChanged={onPlacesChanged}\n          >\n            <div className=\"map-input-box\">\n              <input type=\"text\" placeholder=\"Enter your address\" />\n              <Button type=\"button\" onClick={onConfirm}>\n                Confirm\n              </Button>\n            </div>\n          </StandaloneSearchBox>\n          <Marker position={location} onLoad={onMarkLoad}></Marker>\n        </GoogleMap>\n      </LoadScript>\n    </div>\n  );\n};\n\nexport default MapScreen;\n","/Users/shitalrayamajhi/Documents/Amazon/frontend/src/screens/ResetPasswordScreen.js",[],[],"/Users/shitalrayamajhi/Documents/Amazon/frontend/src/components/AdminRoute.js",[],[],"/Users/shitalrayamajhi/Documents/Amazon/frontend/src/components/ProtectedRoute.js",[],[],"/Users/shitalrayamajhi/Documents/Amazon/frontend/src/components/SearchBox.js",[],[],"/Users/shitalrayamajhi/Documents/Amazon/frontend/src/components/LoadingBox.js",[],[],"/Users/shitalrayamajhi/Documents/Amazon/frontend/src/components/Rating.js",[],[],"/Users/shitalrayamajhi/Documents/Amazon/frontend/src/components/MessageBox.js",[],[],"/Users/shitalrayamajhi/Documents/Amazon/frontend/src/components/Product.js",[],[],"/Users/shitalrayamajhi/Documents/Amazon/frontend/src/components/CheckOutSteps.js",[],[],{"ruleId":"260","severity":1,"message":"261","line":58,"column":10,"nodeType":"262","messageId":"263","endLine":58,"endColumn":23},{"ruleId":"264","replacedBy":"265"},{"ruleId":"260","severity":1,"message":"266","line":21,"column":10,"nodeType":"262","messageId":"263","endLine":21,"endColumn":22},"no-unused-vars","'selectedImage' is assigned a value but never used.","Identifier","unusedVar","no-new-object",["267"],"'googleApiKey' is assigned a value but never used.","no-object-constructor"]